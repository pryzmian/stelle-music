{
    "name": "stelle-music",
    "version": "0.3.8-BLAZER",
    "description": "A music bot.",
    "main": "./dist/index.js",
    "type": "module",
    "packageManager": "pnpm@10.26.1+sha512.664074abc367d2c9324fdc18037097ce0a8f126034160f709928e9e9f95d98714347044e5c3164d65bd5da6c59c6be362b107546292a8eecb7999196e5ce58fa",
    "homepage": "https://github.com/Ganyu-Studios/stelle-music#readme",
    "author": "Ganyu Studios",
    "license": "MIT",
    "keywords": [
        "discord",
        "discord.js",
        "seyfert",
        "music",
        "bot",
        "discord bot",
        "stelle",
        "lavalink",
        "lavalink-client"
    ],
    "contributors": [
        {
            "name": "JustEvil",
            "url": "https://github.com/EvilG-MC"
        }
    ],
    "engines": {
        "node": ">= 22.11.0",
        "pnpm": ">= 10.7.0"
    },
    "scripts": {
        "build": "tsc",
        "typecheck": "tsc --noEmit",
        "clean": "node ./scripts/clean.mjs && pnpm build",
        "start": "node ./dist/index.js",
        "dev": "tsx ./src/index.ts --debug --dev",
        "lint": "biome lint --write .",
        "format": "biome check --write .",
        "db:generate": "prisma generate",
        "db:push": "prisma db push",
        "db": "pnpm db:push && pnpm db:generate",
        "prepare": "husky"
    },
    "lint-staged": {
        "{*.ts,*.json}": [
            "pnpm format"
        ]
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/Ganyu-Studios/stelle-music.git"
    },
    "bugs": {
        "url": "https://github.com/Ganyu-Studios/stelle-music/issues"
    },
    "dependencies": {
        "@prisma/client": "^6.19.1",
        "dotenv": "^17.2.3",
        "ioredis": "^5.8.2",
        "lavalink-client": "github:tomato6966/lavalink-client",
        "meowdb": "^2.2.3",
        "seyfert": "github:tiramisulabs/seyfert",
        "yunaforseyfert": "^1.1.3"
    },
    "devDependencies": {
        "@biomejs/biome": "^2.3.10",
        "@types/node": "^25.0.3",
        "husky": "^9.1.7",
        "lint-staged": "^16.2.7",
        "prisma": "^6.19.1",
        "tsx": "^4.21.0",
        "typescript": "^5.9.3"
    },
    "imports": {
        "#stelle/classes/*": "./dist/structures/classes/*",
        "#stelle/utils/*": "./dist/structures/utils/*",
        "#stelle/controllers/*": "./dist/structures/controllers/*",
        "#stelle/types": "./dist/structures/types/index.js",
        "#stelle/middlewares": "./dist/middlewares/index.js",
        "#stelle/client": "./dist/index.js",
        "#stelle/prisma": "./dist/generated/prisma/client.js"
    },
    "pnpm": {
        "onlyBuiltDependencies": [
            "@biomejs/biome",
            "@prisma/client",
            "@prisma/engines",
            "esbuild",
            "prisma",
            "lavalink-client",
            "seyfert"
        ]
    }
}
